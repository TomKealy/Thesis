pmid = {23185417},
title = {{Alterations in cerebrospinal fluid proteins in a presymptomatic primary glioma model.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3501526\&tool=pmcentrez\&rendertype=abstract},
volume = {7},
