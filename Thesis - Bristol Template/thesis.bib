eprint = {1502.0376},
file = {:home/tk12098/Documents/Papers/1502.03762.pdf:pdf},
keywords = {information theory,markov,observation channels,optimization,stochastic control},
month = feb,
pages = {17},
title = {{Accelerating a water maser face-on jet from a high mass young stellar object}},
url = {http://arxiv.org/abs/1502.0376},
year = {2015}
