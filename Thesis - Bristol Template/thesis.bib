@article{Montanari2014,
abstract = {We consider the following detection problem: given a realization of a symmetric matrix \$\{\backslash mathbf\{X\}\}\$ of dimension \$n\$, distinguish between the hypothesis that all upper triangular variables are i.i.d. Gaussians variables with mean 0 and variance \$1\$ and the hypothesis where \$\{\backslash mathbf\{X\}\}\$ is the sum of such matrix and an independent rank-one perturbation.   This setup applies to the situation where under the alternative, there is a planted principal submatrix \$\{\backslash mathbf\{B\}\}\$ of size \$L\$ for which all upper triangular variables are i.i.d. Gaussians with mean \$1\$ and variance \$1\$, whereas all other upper triangular elements of \$\{\backslash mathbf\{X\}\}\$ not in \$\{\backslash mathbf\{B\}\}\$ are i.i.d. Gaussians variables with mean 0 and variance \$1\$. We refer to this as the `Gaussian hidden clique problem.'   When \$L=(1+\backslash epsilon)\backslash sqrt\{n\}\$ (\$\backslash epsilon>0\$), it is possible to solve this detection problem with probability \$1-o\_n(1)\$ by computing the spectrum of \$\{\backslash mathbf\{X\}\}\$ and considering the largest eigenvalue of \$\{\backslash mathbf\{X\}\}\$. We prove that this condition is tight in the following sense: when \$L<(1-\backslash epsilon)\backslash sqrt\{n\}\$ no algorithm that examines only the eigenvalues of \$\{\backslash mathbf\{X\}\}\$ can detect the existence of a hidden Gaussian clique, with error probability vanishing as \$n\backslash to\backslash infty\$.   We prove this result as an immediate consequence of a more general result on rank-one perturbations of \$k\$-dimensional Gaussian tensors. In this context we establish a lower bound on the critical signal-to-noise ratio below which a rank-one signal cannot be detected.},
archivePrefix = {arXiv},
arxivId = {1411.6149},
author = {Montanari, Andrea and Reichman, Daniel and Zeitouni, Ofer},
eprint = {1411.6149},
file = {:home/tk12098/Documents/Papers/1411.6149.pdf:pdf},
month = nov,
number = {621},
pages = {15},
title = {{On the limitation of spectral methods: From the Gaussian hidden clique problem to rank one perturbations of Gaussian tensors}},
url = {http://arxiv.org/abs/1411.6149},
year = {2014}
}
