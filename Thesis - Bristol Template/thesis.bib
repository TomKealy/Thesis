@article{Brown2014,
abstract = {In this paper we shall re-visit the well-known Schr$\backslash$"odinger and Lindblad dynamics of quantum mechanics. However, these equations may be realized as the consequence of a more general, underlying dynamical process. In both cases we shall see that the evolution of a quantum state \$P\_\backslash psi=\backslash varrho(0)\$ has the not so well-known pseudo-quadratic form \$\backslash partial\_t\backslash varrho(t)=\backslash mathbf\{V\}\^{}\backslash star\backslash varrho(t)\backslash mathbf\{V\}\$ where \$\backslash mathbf\{V\}\$ is a vector operator in a complex Minkowski space and the pseudo-adjoint \$\backslash mathbf\{V\}\^{}\backslash star\$ is induced by the Minkowski metric \$\backslash boldsymbol\{\backslash eta\}\$. The interesting thing about this formalism is that its derivation has very deep roots in a new understanding of the differential calculus of time. This Minkowski-Hilbert representation of quantum dynamics is called the $\backslash$emph\{Belavkin Formalism\}; a beautiful, but not well understood theory of mathematical physics that understands that both deterministic and stochastic dynamics may be `unraveled' in a second-quantized Minkowski space. Working in such a space provided the author with the means to construct a QS (quantum stochastic) Duhamel principle and known applications to a Schr$\backslash$"odinger dynamics perturbed by a continual measurement process are considered. What is not known, but presented here, is the role of the Lorentz transform in quantum measurement, and the appearance of Riemannian geometry in quantum measurement is also discussed.},
archivePrefix = {arXiv},
arxivId = {1407.2875},
author = {Brown, Matthew F.},
eprint = {1407.2875},
file = {:home/tk12098/Documents/Papers/1407.2875.pdf:pdf},
month = jul,
pages = {1--33},
title = {{Quantum Dynamics, Minkowski-Hilbert space, and A Quantum Stochastic Duhamel Principle}},
url = {http://arxiv.org/abs/1407.2875},
year = {2014}
}
