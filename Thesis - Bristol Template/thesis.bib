pages = {12},
title = {{Transit Timing Observations from Kepler: VII. Confirmation of 27 planets in 13 multiplanet systems via Transit Timing Variations and orbital stability}},
url = {http://arxiv.org/abs/1208.3499},
volume = {364},
year = {2012}
