year = {2012}
}
@article{Cranston2015,
abstract = {The \{$\backslash$em square\} \$G\^{}2\$ of a graph \$G\$ is the graph with the same vertex set as \$G\$ and with two vertices adjacent if their distance in \$G\$ is at most 2. Thomassen showed that every planar graph \$G\$ with maximum degree \$\backslash Delta(G)=3\$ satisfies \$\backslash chi(G\^{}2)\backslash leq 7\$. Kostochka and Woodall conjectured that for every graph, the list-chromatic number of \$G\^{}2\$ equals the chromatic number of \$G\^{}2\$, that is \$\backslash chi\_l(G\^{}2)=\backslash chi(G\^{}2)\$ for all \$G\$. If true, this conjecture (together with Thomassen's result) implies that every planar graph \$G\$ with \$\backslash Delta(G)=3\$ satisfies \$\backslash chi\_l(G\^{}2)\backslash leq 7\$. We prove that every connected graph (not necessarily planar) with \$\backslash Delta(G)=3\$ other than the Petersen graph satisfies \$\backslash chi\_l(G\^{}2)\backslash leq 8\$ (and this is best possible). In addition, we show that if \$G\$ is a planar graph with \$\backslash Delta(G)=3\$ and girth \$g(G)\backslash geq 7\$, then \$\backslash chi\_l(G\^{}2)\backslash leq 7\$. Dvo$\backslash$v\{r\}$\backslash$'ak, $\backslash$v\{S\}krekovski, and Tancer showed that if \$G\$ is a planar graph with \$\backslash Delta(G) = 3\$ and girth \$g(G) \backslash geq 10\$, then \$\backslash chi\_l(G\^{}2)\backslash leq 6\$. We improve the girth bound to show that if \$G\$ is a planar graph with \$\backslash Delta(G)=3\$ and \$g(G) \backslash geq 9\$, then \$\backslash chi\_l(G\^{}2) \backslash leq 6\$. All of our proofs can be easily translated into linear-time coloring algorithms.},
archivePrefix = {arXiv},
arxivId = {1503.0157},
author = {Cranston, Daniel W. and Kim, Seog-Jin},
eprint = {1503.0157},
file = {:home/tk12098/Documents/Papers/1503.01570.pdf:pdf},
keywords = {bernoulli sums,concavity,entropy,poisson binomial dis-,transportation of measures,tribution},
month = feb,
pages = {1--12},
title = {{List-coloring the Square of a Subcubic Graph}},
url = {http://arxiv.org/abs/1503.0157},
year = {2015}
}
@article{Fotouhi2014,
abstract = {The friendship paradox refers to the sociological observation that, while the people's assessment of their own popularity is typically self-aggrandizing, in reality they are less popular than their friends. The generalized friendship paradox is the average alter superiority observed empirically in social settings, scientific collaboration networks, as well as online social media. We posit a quality-based network growth model in which the chance for a node to receive new links depends both on its degree and a quality parameter. Nodes are assigned qualities the first time they join the network, and these do not change over time. We analyse the model theoretically, finding expressions for the joint degree-quality distribution and nearest-neighbor distribution. We then demonstrate that this model exhibits both the friendship paradox and the generalized friendship paradox at the network level, regardless of the distribution of qualities. We also show that, in the proposed model, the degree and quality of each node are positively correlated regardless of how node qualities are distributed.},
archivePrefix = {arXiv},
arxivId = {1410.0586},
author = {Fotouhi, Babak and Momeni, Naghmeh and Rabbat, Michael G.},
eprint = {1410.0586},
file = {:home/tk12098/Documents/Papers/1410.0586.pdf:pdf},
month = oct,
title = {{Generalized Friendship Paradox: An Analytical Approach}},
