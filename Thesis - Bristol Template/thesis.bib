}
@article{Ejlli2015,
abstract = {Axion production due to photon-axion mixing in tangled magnetic field(s) prior to recombination epoch and magnetic field damping can generate cosmic microwave background (CMB) spectral distortions. In particular, contribution of both processes to CMB \$\backslash mu\$ distortion in the case of resonant photon-axion mixing is studied. Assuming that magnetic field power spectrum is approximated by a power law \$P\_B(k)\backslash propto k\^{}n\$ with spectral index \$n\$, it is shown that for magnetic field cut-off scales \$172.5\$ pc \$\backslash leq \backslash lambda\_B\backslash leq 4\backslash times 10\^{}3\$ pc, axion contribution to CMB \$\backslash mu\$ distortion is subdominant in comparison with magnetic field damping in the cosmological plasma. Using COBE upper limit on \$\backslash mu\$ and for magnetic field scale \$\backslash lambda\_B\backslash simeq 415\$ pc, weaker limit in comparison with other studies on the magnetic field strength (\$B\_0\backslash leq 8.5\backslash times 10\^{}\{-8\}\$ G) up to a factor 10 for the DFSZ axion model and axion mass \$m\_a\backslash geq 2.6\backslash times 10\^{}\{-6\}\$ eV is found. A forecast for the expected sensitivity of PIXIE/PRISM on \$\backslash mu\$ is also presented.},
