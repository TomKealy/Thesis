file = {:home/tk12098/Documents/Papers/1507.05021.pdf:pdf},
keywords = {and phrases,carlo,langevin diffusion,markov chain monte,metropolis adjusted langevin algorithm,rate of convergence,wasserstein distance},
month = jul,
number = {2},
title = {{Effect of Stellar Encounters on Comet Cloud Formation}},
url = {http://arxiv.org/abs/1507.0502},
volume = {1},
year = {2015}
}
