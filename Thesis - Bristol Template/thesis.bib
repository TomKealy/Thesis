file = {:home/tk12098/Documents/Papers/1507.05053.pdf:pdf},
keywords = {ann,artificial neural networks,as it,boltzmann machine,dataset the size of,fit in the,gpu,graph-,ics processing unit,mnist,mnist handwritten database 1,rbm,restrictive,s too large to},
