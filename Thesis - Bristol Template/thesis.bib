year = {2015}
}
@article{Gravier2015,
abstract = {The \{$\backslash$em Fibonacci cube\} of dimension \$n\$, denoted as \$\backslash Gamma\backslash\_n\$, is the subgraph of \$n\$-cube \$Q\backslash\_n\$ induced by vertices with no consecutive 1's. We study the maximum number of disjoint subgraphs in \$\backslash Gamma\backslash\_n\$ isomorphic to \$Q\backslash\_k\$, and denote this number by \$q\backslash\_k(n)\$. We prove several recursive results for \$q\backslash\_k(n)\$, in particular we prove that \$q\backslash\_\{k\}(n) = q\backslash\_\{k-1\}(n-2) + q\backslash\_\{k\}(n-3)\$. We also prove a closed formula in which \$q\backslash\_k(n)\$ is given in terms of Fibonacci numbers, and finally we give the generating function for the sequence \$\backslash\{q\backslash\_\{k\}(n)\backslash\}\backslash\_\{n=0\}\^{}\{ \backslash infty\}\$.},
