@article{Johnson2009a,
abstract = {Sufficient conditions are developed, under which the compound Poisson distribution has maximal entropy within a natural class of probability measures on the nonnegative integers. Recently, one of the authors [O. Johnson, \{$\backslash$em Stoch. Proc. Appl.\}, 2007] used a semigroup approach to show that the Poisson has maximal entropy among all ultra-log-concave distributions with fixed mean. We show via a non-trivial extension of this semigroup approach that the natural analog of the Poisson maximum entropy property remains valid if the compound Poisson distributions under consideration are log-concave, but that it fails in general. A parallel maximum entropy result is established for the family of compound binomial measures. Sufficient conditions for compound distributions to be log-concave are discussed and applications to combinatorics are examined; new bounds are derived on the entropy of the cardinality of a random independent set in a claw-free graph, and a connection is drawn to Mason's conjecture for matroids. The present results are primarily motivated by the desire to provide an information-theoretic foundation for compound Poisson approximation and associated limit theorems, analogous to the corresponding developments for the central limit theorem and for Poisson approximation. Our results also demonstrate new links between some probabilistic methods and the combinatorial notions of log-concavity and ultra-log-concavity, and they add to the growing body of work exploring the applications of maximum entropy characterizations to problems in discrete mathematics.},
archivePrefix = {arXiv},
arxivId = {0912.0581},
author = {Johnson, Oliver and Kontoyiannis, Ioannis and Madiman, Mokshay},
doi = {10.1016/j.dam.2011.08.025},
eprint = {0912.0581},
file = {:home/tk12098/Documents/Papers/0912.0581.pdf:pdf},
keywords = {compound poisson distribution,log-concavity,maximum entropy,ultra-log-concavity},
month = dec,
pages = {30},
title = {{Log-concavity, ultra-log-concavity, and a maximum entropy property of discrete compound Poisson measures}},
url = {http://arxiv.org/abs/0912.0581},
year = {2009}
}
@article{Acharya2014,
abstract = {It was recently shown that estimating the Shannon entropy \$H(\{\backslash rm p\})\$ of a discrete \$k\$-symbol distribution \$\{\backslash rm p\}\$ requires \$\backslash Theta(k/\backslash log k)\$ samples, a number that grows near-linearly in the support size. In many applications \$H(\{\backslash rm p\})\$ can be replaced by the more general R$\backslash$'enyi entropy of order \$\backslash alpha\$, \$H\_\backslash alpha(\{\backslash rm p\})\$. We determine the number of samples needed to estimate \$H\_\backslash alpha(\{\backslash rm p\})\$ for all \$\backslash alpha\$, showing that \$\backslash alpha < 1\$ requires a super-linear, roughly \$k\^{}\{1/\backslash alpha\}\$ samples, noninteger \$\backslash alpha>1\$ requires a near-linear \$k\$ samples, but, perhaps surprisingly, integer \$\backslash alpha>1\$ requires only \$\backslash Theta(k\^{}\{1-1/\backslash alpha\})\$ samples. Furthermore, developing on a recently established connection between polynomial approximation and estimation of additive functions of the form \$\backslash sum\_\{x\} f(\{\backslash rm p\}\_x)\$, we reduce the sample complexity for noninteger values of \$\backslash alpha\$ by a factor of \$\backslash log k\$ compared to the empirical estimator. The estimators achieving these bounds are simple and run in time linear in the number of samples. Our lower bounds provide explicit constructions of distributions with different R$\backslash$'enyi entropies that are hard to distinguish.},
archivePrefix = {arXiv},
arxivId = {1408.1000},
author = {Acharya, Jayadev and Orlitsky, Alon and Suresh, Ananda Theertha and Tyagi, Himanshu},
eprint = {1408.1000},
file = {:home/tk12098/Documents/Papers/1408.1000.pdf:pdf},
month = aug,
title = {{Estimating Renyi Entropy of Discrete Distributions}},
url = {http://arxiv.org/abs/1408.1000},
year = {2014}
}
