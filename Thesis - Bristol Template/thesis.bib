@article{Zhang2012a,
abstract = {This paper shows that the solutions to various convex \$\backslash ell\_1\$ minimization problems are $\backslash$emph\{unique\} if and only if a common set of conditions are satisfied. This result applies broadly to the basis pursuit model, basis pursuit denoising model, Lasso model, as well as other \$\backslash ell\_1\$ models that either minimize \$f(Ax-b)\$ or impose the constraint \$f(Ax-b)\backslash leq\backslash sigma\$, where \$f\$ is a strictly convex function. For these models, this paper proves that, given a solution \$x\^{}*\$ and defining \$I=\backslash supp(x\^{}*)\$ and \$s=\backslash sign(x\^{}*\_I)\$, \$x\^{}*\$ is the unique solution if and only if \$A\_I\$ has full column rank and there exists \$y\$ such that \$A\_I\^{}Ty=s\$ and \$|a\_i\^{}Ty|\_\backslash infty<1\$ for \$i\backslash not\backslash in I\$. This condition is previously known to be sufficient for the basis pursuit model to have a unique solution supported on \$I\$. Indeed, it is also necessary, and applies to a variety of other \$\backslash ell\_1\$ models. The paper also discusses ways to recognize unique solutions and verify the uniqueness conditions numerically.},
archivePrefix = {arXiv},
arxivId = {1209.0652},
