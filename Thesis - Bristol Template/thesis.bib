@article{Panagiotou2014,
abstract = {We study the uniform random graph \$\backslash mathsf\{C\}\_n\$ with \$n\$ vertices drawn from a subcritical class of connected graphs. Our main result is that the rescaled graph \$\backslash mathsf\{C\}\_n / \backslash sqrt\{n\}\$ converges to the Brownian Continuum Random Tree \$\backslash mathcal\{T\}\_\{\backslash mathsf\{e\}\}\$ multiplied by a constant scaling factor that depends on the class under consideration. In addition, we provide subgaussian tail bounds for the diameter \$\backslash text\{D\}(\backslash mathsf\{C\}\_n)\$ and height \$\backslash text\{H\}(\backslash mathsf\{C\}\_n\^{}\backslash bullet)\$ of the rooted random graph \$\backslash mathsf\{C\}\_n\^{}\backslash bullet\$. We give analytic expressions for the scaling factor of several classes, including for example the prominent class of outerplanar graphs. Our methods also enable us to study first passage percolation on \$\backslash mathsf\{C\}\_n\$, where we show the convergence to \$\backslash mathcal\{T\}\_\{\backslash mathsf\{e\}\}\$ under an appropriate rescaling.},
archivePrefix = {arXiv},
