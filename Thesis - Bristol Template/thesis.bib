@article{Shpilka2013,
abstract = {In this paper we prove results regarding Boolean functions with small spectral norm (the spectral norm of f is \$\backslash|\backslash hat\{f\}\backslash|\_1=\backslash sum\_\{\backslash alpha\}|\backslash hat\{f\}(\backslash alpha)|\$). Specifically, we prove the following results for functions \$f:\backslash\{0,1\backslash\}\^{}n \backslash to \backslash\{0,1\backslash\}\$ with \$\backslash|\backslash hat\{f\}\backslash|\_1=A\$.   1. There is a subspace \$V\$ of co-dimension at most \$A\^{}2\$ such that \$f|\_V\$ is constant.   2. f can be computed by a parity decision tree of size \$2\^{}\{A\^{}2\}n\^{}\{2A\}\$. (a parity decision tree is a decision tree whose nodes are labeled with arbitrary linear functions.)   3. If in addition f has at most s nonzero Fourier coefficients, then f can be computed by a parity decision tree of depth \$A\^{}2 \backslash log s\$.   4. For every \$0<\backslash epsilon\$ there is a parity decision tree of depth \$O(A\^{}2 + \backslash log(1/\backslash epsilon))\$ and size \$2\^{}\{O(A\^{}2)\} \backslash cdot \backslash min\backslash\{1/\backslash epsilon\^{}2,O(\backslash log(1/\backslash epsilon))\^{}\{2A\}\backslash\}\$ that $\backslash$epsilon-approximates f. Furthermore, this tree can be learned, with probability \$1-\backslash delta\$, using \$\backslash poly(n,\backslash exp(A\^{}2),1/\backslash epsilon,\backslash log(1/\backslash delta))\$ membership queries.   All the results above also hold (with a slight change in parameters) to functions \$f:Z\_p\^{}n\backslash to \backslash\{0,1\backslash\}\$.},
archivePrefix = {arXiv},
arxivId = {1304.0371},
author = {Shpilka, Amir and Tal, Avishay and Volk, Ben Lee},
eprint = {1304.0371},
file = {:home/tk12098/Documents/Papers/1304.0371v2.pdf:pdf},
