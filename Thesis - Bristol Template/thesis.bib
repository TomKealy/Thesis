month = jun,
pages = {1--40},
primaryClass = {math},
title = {{Sharp Metastability Threshold for Two-Dimensional Bootstrap Percolation}},
url = {http://arxiv.org/abs/math/0206132},
volume = {2002},
year = {2002}
}
