title = {{Blind Denoising with Random Greedy Pursuits}},
url = {http://arxiv.org/abs/1312.5444},
year = {2013}
}
@article{Holroyd2002,
abstract = {In the bootstrap percolation model, sites in an \$L\$ by \$L\$ square are initially independently declared active with probability \$p\$. At each time step, an inactive site becomes active if at least two of its four neighbours are active. We study the behaviour as \$p \backslash to 0\$ and \$L \backslash to \backslash infty\$ simultaneously of the probability \$I(L,p)\$ that the entire square is eventually active. We prove that \$I(L,p) \backslash to 1\$ if \$\backslash liminf p \backslash log L > \backslash lambda\$, and \$I(L,p) \backslash to 0\$ if \$\backslash limsup p \backslash log L < \backslash lambda\$, where \$\backslash lambda = \backslash pi\^{}2/18\$. We prove the same behaviour, with the same threshold \$\backslash lambda\$, for the probability \$J(L,p)\$ that a site is active by time \$L\$ in the process on the infinite lattice. The same results hold for the so-called modified bootstrap percolation model, but with threshold \$\backslash lambda' = \backslash pi\^{}2/6\$. The existence of the thresholds \$\backslash lambda,\backslash lambda'\$ settles a conjecture of Aizenman and Lebowitz, while the determination of their values corrects numerical predictions of Adler, Stauffer and Aharony.},
archivePrefix = {arXiv},
arxivId = {math/0206132},
author = {Holroyd, Alexander E.},
