@article{Guruswami2014,
abstract = {We prove a lower estimate on the increase in entropy when two copies of a conditional random variable \$X | Y\$, with \$X\$ supported on \$\backslash mathbb\{Z\}\_q=\backslash\{0,1,\backslash dots,q-1\backslash\}\$ for prime \$q\$, are summed modulo \$q\$. Specifically, given two i.i.d copies \$(X\_1,Y\_1)\$ and \$(X\_2,Y\_2)\$ of a pair of random variables \$(X,Y)\$, with \$X\$ taking values in \$\backslash mathbb\{Z\}\_q\$, we show $\backslash$[ H(X\_1 + X\_2 $\backslash$mid Y\_1, Y\_2) - H(X|Y) $\backslash$ge $\backslash$alpha(q) $\backslash$cdot H(X|Y) (1-H(X|Y)) $\backslash$] for some \$\backslash alpha(q) > 0\$, where \$H(\backslash cdot)\$ is the normalized (by factor \$\backslash log\_2 q\$) entropy. Our motivation is an effective analysis of the finite-length behavior of polar codes, and the assumption of \$q\$ being prime is necessary. For \$X\$ supported on infinite groups without a finite subgroup and no conditioning, a sumset inequality for the absolute increase in (unnormalized) entropy was shown by Tao (2010).   We use our sumset inequality to analyze Ar\{$\backslash$i\}kan's construction of polar codes and prove that for any \$q\$-ary source \$X\$, where \$q\$ is any fixed prime, and any \$\backslash epsilon > 0\$, polar codes allow \{$\backslash$em efficient\} data compression of \$N\$ i.i.d. copies of \$X\$ into \$(H(X)+\backslash epsilon)N\$ \$q\$-ary symbols, as soon as \$N\$ is polynomially large in \$1/\backslash epsilon\$. We can get capacity-achieving source codes with similar guarantees for composite alphabets, by factoring \$q\$ into primes and combining different polar codes for each prime in factorization.   A consequence of our result for noisy channel coding is that for \{$\backslash$em all\} discrete memoryless channels, there are explicit codes enabling reliable communication within \$\backslash epsilon > 0\$ of the symmetric Shannon capacity for a block length and decoding complexity bounded by a polynomial in \$1/\backslash epsilon\$. The result was previously shown for the special case of binary input channels (Guruswami-Xia '13 and Hassani-Alishahi-Urbanke '13), and this work extends the result to channels over any alphabet.},
archivePrefix = {arXiv},
arxivId = {1411.6993},
author = {Guruswami, Venkatesan and Velingker, Ameya},
eprint = {1411.6993},
file = {:home/tk12098/Documents/Papers/1411.6993.pdf:pdf},
month = nov,
title = {{An Entropy Sumset Inequality and Polynomially Fast Convergence to Shannon Capacity Over All Alphabets}},
url = {http://arxiv.org/abs/1411.6993},
year = {2014}
}
@article{Quan2009,
abstract = {Spectrum sensing is one of the enabling functionalities for cognitive radio (CR) systems to operate in the spectrum white space. To protect the primary incumbent users from interference, the CR is required to detect incumbent signals at very low signal-to-noise ratio (SNR). In this paper, we present a spectrum sensing technique based on correlating spectra for detection of television (TV) broadcasting signals. The basic strategy is to correlate the periodogram of the received signal with the a priori known spectral features of the primary signal. We show that according to the Neyman-Pearson criterion, this spectral correlation-based sensing technique is asymptotically optimal at very low SNR and with a large sensing time. From the system design perspective, we analyze the effect of the spectral features on the spectrum sensing performance. Through the optimization analysis, we obtain useful insights on how to choose effective spectral features to achieve reliable sensing. Simulation results show that the proposed sensing technique can reliably detect analog and digital TV signals at SNR as low as -20 dB.},
archivePrefix = {arXiv},
arxivId = {0907.1992},
author = {Quan, Zhi and Shellhammer, Stephen J. and Zhang, Wenyi and Sayed, Ali H.},
eprint = {0907.1992},
file = {:home/tk12098/Documents/Papers/0907.1992.pdf:pdf},
month = jul,
title = {{Spectrum sensing by cognitive radios at very low SNR}},
url = {http://arxiv.org/abs/0907.1992},
year = {2009}
}
