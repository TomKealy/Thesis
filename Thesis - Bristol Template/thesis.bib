file = {:home/tk12098/Documents/Papers/1506.08187.pdf:pdf},
month = jun,
number = {2014},
pages = {25},
title = {{ALMA observations of TiO\$\_2\$ around VY Canis Majoris}},
url = {http://arxiv.org/abs/1506.0818},
year = {2015}
}
