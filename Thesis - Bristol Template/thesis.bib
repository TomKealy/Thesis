author = {Churazov, E. and Sunyaev, R. and Isern, J. and Bikmaev, I. and Bravo, E. and Chugai, N. and Grebenev, S. and Jean, P. and Kn\"{o}dlseder, J. and Lebrun, F. and Kuulkers, E.},
