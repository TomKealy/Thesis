year = {2014}
}
@article{Kaji2015a,
abstract = {In this paper we use umkehr maps to describe a certain product \$\backslash mu\$ in homology which arises in some natural settings. We apply it to define a string product in the generalized homology of the Borel construction of a smooth manifold \$M\$ with a smooth action of a compact Lie group \$G\$ with an appropriate choice of orientation. This string product reduces to the Chas-Sullivan product when \$G\$ is trivial and to the Chataur-Menichi product when \$M\$ is a point. In the case of the ordinary homology, we prove a vanishing result for \$\backslash mu\$ which enables us to define a secondary product \$\backslash overline\{\backslash mu\}\$ under certain conditions. We use \$\backslash bar\{\backslash mu\}\$ to introduce a secondary exterior product in the Borel equivariant homology and a secondary intersection product in the Borel equivariant homology of \$M\$. These secondary products generalize the product in the homology of \$BG\$ defined by Kreck which was shown to coincide with the product in negative Tate cohomology in the case where \$G\$ is finite. We study those products and prove some vanishing results. We also indicate that they are non trivial in many cases.},
archivePrefix = {arXiv},
