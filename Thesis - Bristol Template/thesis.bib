@article{Polyanskiy2013,
abstract = {Consider a linear space of functions on the binary hypercube and a linear operator \$T\_\backslash delta\$ acting by averaging a function over a Hamming sphere of radius \$\backslash delta n\$. It is shown that such an operator has a dimension independent bound on the norm \$L\_p \backslash to L\_2\$ with \$p = 1+(1-2\backslash delta)\^{}2\$. This result evidently parallels a classical estimate of Bonami and Gross for \$L\_p \backslash to L\_q\$ norms for the operator of convolution with a Bernoulli noise. The estimate for \$T\_\backslash delta\$ is harder to obtain since the latter is neither a part of a semigroup, nor a tensor power. The result is shown by a detailed study of the eigenvalues of \$T\_\backslash delta\$ and \$L\_p\backslash to L\_2\$ norms of the Fourier multiplier operators \$\backslash Pi\_a\$ with symbol equal to a characterstic function of the Hamming sphere of radius \$a\$.   An application of the result to additive combinatorics is given: Any set \$A\backslash subset \backslash FF\_2\^{}n\$ with the property that \$A+A\$ contains a large portion of some Hamming sphere (counted with multiplicity) must have cardinality a constant multiple of \$2\^{}n\$. It is also demonstrated that this result does not follow from standard spectral gap and semi-definite (Lov$\backslash$'asz-Delsarte) methods.},
archivePrefix = {arXiv},
arxivId = {1309.3014},
author = {Polyanskiy, Yury},
eprint = {1309.3014},
file = {:home/tk12098/Documents/Papers/1309.3014.pdf:pdf},
month = sep,
pages = {1--25},
title = {{Hypercontractivity of spherical averages in Hamming space}},
url = {http://arxiv.org/abs/1309.3014},
year = {2013}
}
