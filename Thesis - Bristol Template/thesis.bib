@article{Cowley2014,
abstract = {Recent observational evidence suggests that the coarse angular resolution (\$\backslash sim20"\$ FWHM) of single-dish telescopes at sub-mm wavelengths has biased the observed galaxy number counts by blending together the sub-mm emission from multiple sub-mm galaxies (SMGs). We use lightcones computed from an updated implementation of the $\backslash$galform semi-analytic model to generate \$50\$ mock sub-mm surveys of \$0.5\$ deg\$\^{}2\$ at \$850\$ \$\backslash mu\$m, taking into account the effects of the finite single-dish beam in a more accurate way than has been done previously. We find that blending of SMGs does lead to an enhancement of source extracted number counts at bright fluxes (\$S\_\{\backslash mathrm\{850\}\backslash mu\backslash mathrm\{m\}\}\backslash gtrsim1\$ mJy). Typically, \$\backslash sim3\{-\}6\$ galaxies contribute \$90\backslash\%\$ of the flux of an \$S\_\{850\backslash mu\backslash mathrm\{m\}\}=5\$ mJy source and these blended galaxies are physically unassociated. We find that field-to-field variations are comparable to Poisson fluctuations for our \$S\_\{850\backslash mu\backslash mathrm\{m\}\}>5\$ mJy SMG population, which has a median redshift \$z\_\{50\}=2.0\$, but are greater than Poisson for the \$S\_\{850\backslash mu\backslash mathrm\{m\}\}>1\$ mJy population (\$z\_\{50\}=2.8\$). In a detailed comparison to a recent interferometric survey targeted at single-dish detected sources, we reproduce the difference between single-dish and interferometer number counts and find a median redshift (\$z\_\{50\}=2.5\$) in excellent agreement with the observed value (\$z\_\{50\}=2.5\backslash pm 0.2\$). We also present predictions for single-dish survey number counts at \$450\$ and \$1100\$ \$\backslash mu\$m, which show good agreement with observational data.},
archivePrefix = {arXiv},
arxivId = {1406.0855},
author = {Cowley, William I. and Lacey, Cedric G. and Baugh, Carlton M. and Cole, Shaun},
doi = {10.1093/mnras/stu2179},
eprint = {1406.0855},
file = {:home/tk12098/Documents/Papers/1406.0855.pdf:pdf},
keywords = {galaxies,sub-millimetre},
month = jun,
number = {October},
pages = {15},
title = {{Simulated observations of sub-millimetre galaxies: the impact of single-dish resolution and field variance}},
url = {http://arxiv.org/abs/1406.0855},
volume = {15},
year = {2014}
}
