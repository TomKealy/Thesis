}
@article{Ellis2005,
abstract = {The unit ball random geometric graph \$G=G\^{}d\_p(\backslash lambda,n)\$ has as its vertices \$n\$ points distributed independently and uniformly in the \$d\$-dimensional unit ball, with two vertices adjacent if and only if their \$l\_p\$-distance is at most \$\backslash lambda\$. Like its cousin the Erdos-Renyi random graph, \$G\$ has a connectivity threshold: an asymptotic value for \$\backslash lambda\$ in terms of \$n\$, above which \$G\$ is connected and below which \$G\$ is disconnected (and in fact has isolated vertices in most cases). In the connected zone, we determine upper and lower bounds for the graph diameter of \$G\$. Specifically, almost always, \$\backslash diam\_p(\backslash mathbf\{B\})(1-o(1))/\backslash lambda \backslash leq \backslash diam(G) \backslash leq \backslash diam\_p(\backslash mathbf\{B\})(1+O((\backslash ln \backslash ln n/\backslash ln n)\^{}\{1/d\}))/\backslash lambda\$, where \$\backslash diam\_p(\backslash mathbf\{B\})\$ is the \$\backslash ell\_p\$-diameter of the unit ball \$\backslash mathbf\{B\}\$. We employ a combination of methods from probabilistic combinatorics and stochastic geometry.},
