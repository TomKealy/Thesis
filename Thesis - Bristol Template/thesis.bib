@article{Wang2014a,
abstract = {We consider the quantitative group testing problem where the objective is to identify defective items in a given population based on results of tests performed on subsets of the population. Under the quantitative group testing model, the result of each test reveals the number of defective items in the tested group. The minimum number of tests achievable by nested test plans was established by Aigner and Schughart in 1985 within a minimax framework. The optimal nested test plan offering this performance, however, was not obtained. In this work, we establish the optimal nested test plan in closed form. This optimal nested test plan is also asymptotically (as the population size grows to infinity) optimal among all test plans. We then focus on the application of heavy hitter detection problem for traffic monitoring and anomaly detection in the Internet and other communication networks. For such applications, it is often the case that a few abnormal traffic flows with exceptionally high volume (referred to as heavy hitters) make up most of the traffic seen by the entire network. Since the volume of heavy hitters is much higher than that of normal flows, the number of heavy hitters in a group of flows can be accurately estimated from the aggregated traffic load. Other potential applications include detecting idle channels in the radio spectrum in the high SNR regime.},
archivePrefix = {arXiv},
arxivId = {1407.2283},
author = {Wang, Chao and Zhao, Qing and Chuah, Chen-Nee},
eprint = {1407.2283},
file = {:home/tk12098/Documents/Papers/1407.2283.pdf:pdf},
month = jul,
title = {{Quantitative Group Testing for Heavy Hitter Detection}},
url = {http://arxiv.org/abs/1407.2283},
year = {2014}
}
@article{Castillo2014,
abstract = {We study the statistics of the condition number \$\backslash kappa=\backslash lambda\_\{\backslash mathrm\{max\}\}/\backslash lambda\_\{\backslash mathrm\{min\}\}\$ (the ratio between largest and smallest squared singular values) of \$N\backslash times M\$ Gaussian random matrices. Using a Coulomb fluid technique, we derive analytically and for large \$N\$ the cumulative \$\backslash mathcal\{P\}[\backslash kappa<x]\$ and tail-cumulative \$\backslash mathcal\{P\}[\backslash kappa>x]\$ distributions of \$\backslash kappa\$. We find that these distributions decay as \$\backslash mathcal\{P\}[\backslash kappa<x]\backslash approx\backslash exp\backslash left(-\backslash beta N\^{}2 \backslash Phi\_\{-\}(x)\backslash right)\$ and \$\backslash mathcal\{P\}[\backslash kappa>x]\backslash approx\backslash exp\backslash left(-\backslash beta N \backslash Phi\_\{+\}(x)\backslash right)\$, where \$\backslash beta\$ is the Dyson index of the ensemble. The left and right rate functions \$\backslash Phi\_\{\backslash pm\}(x)\$ are independent of \$\backslash beta\$ and calculated exactly for any choice of the rectangularity parameter \$\backslash alpha=M/N-1>0\$. Interestingly, they show a weak non-analytic behavior at their minimum \$\backslash langle\backslash kappa\backslash rangle\$ (corresponding to the average condition number), a direct consequence of a phase transition in the associated Coulomb fluid problem. Matching the behavior of the rate functions around \$\backslash langle\backslash kappa\backslash rangle\$, we determine exactly the scale of typical fluctuations \$\backslash sim\backslash mathcal\{O\}(N\^{}\{-2/3\})\$ and the tails of the limiting distribution of \$\backslash kappa\$. The analytical results are in excellent agreement with numerical simulations.},
archivePrefix = {arXiv},
arxivId = {1403.1185},
author = {Castillo, Isaac P\'{e}rez and Katzav, Eytan and Vivo, Pierpaolo},
doi = {10.1103/PhysRevE.90.050103},
eprint = {1403.1185},
file = {:home/tk12098/Documents/Papers/1403.1185.pdf:pdf},
month = mar,
pages = {1--12},
title = {{Phase transitions in the condition number distribution of Gaussian random matrices}},
url = {http://arxiv.org/abs/1403.1185},
year = {2014}
}
@article{Moeckli2015,
abstract = {We generalize the Chebyshev-Bogoliubov-deGennes method to treat multi-band systems to address the temperature dependence of the superconducting (SC) gaps of iron based superconductors. Four SC gaps associated with different electron and hole pockets of optimally doped Ba\$\_\{0.6\}\$K\$\_\{0.4\}\$Fe\$\_2\$As\$\_2\$ were clearly identified by angle resolved photo-emission spectroscopy. The few approaches that reproduces with success this gap structure are based on strong-coupling theories and required many adjustable parameters. We show that an approach with a redistribution of electron population between the hole and electron pockets \$\backslash nu\$ with evolving temperature reproduces the different coupling ratios \$2\backslash Delta\^{}\{\backslash nu\}(0)/k\_\{\backslash rm B\} T\_c\$ in these materials. We define the values that fit the four zero temperature gaps \$\backslash Delta\^{}\{\backslash nu\}(0)\$ and after that all \$\backslash Delta\^{}\{\backslash nu\}(T)\$ is obtained without any additional parameter.},
