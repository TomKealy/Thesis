file = {:home/tk12098/Documents/Papers/1412.4187.pdf:pdf},
keywords = {diffusion approximation,generalized jackson network,heavy traffic,kingman,multi-class,queueing network,s approximation,semi-martingale reflecting brownian motion,single-class,sta-,tail asymptotics,tionary distribution},
month = dec,
title = {{Diffusion approximation for stationary analysis of queues and their networks: A review}},
