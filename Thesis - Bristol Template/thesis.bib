year = {2011}
}
@article{Cox2015,
abstract = {We develop the basic theory of $\backslash$emph\{layered posets\} and prove a general iteration theorem about the $\backslash$emph\{universal Kunen collapse\}. We prove that the \$\backslash kappa\$-stationary layering property implies the \$\backslash kappa\$-Knaster property, and use the notion of layering to prove: if \$\backslash kappa\$ is weakly compact then $\backslash$emph\{$\backslash$textbf\{any\}\} Kunen-style universal iteration of \$\backslash kappa\$-cc posets (each possibly of size \$\backslash kappa\$) is \$\backslash kappa\$-cc, as long as direct limits are used sufficiently often. This theorem simplifies and generalizes the various chain condition arguments in the literature on saturated ideals, in cases where finite support iterations are not possible.},
